<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dch.bean.Dymess" table="dy_mess">
		<id name="id" column="id" type="string">
			<generator class="uuid.hex"></generator>
		</id>
		<property name="title" column="title" type="string"></property>
		<property name="headpng" column="headpng" type="string"></property>
		<property name="context" column="context" type="string"></property>
		<property name="crdatetime" column="crdatetime" type="java.util.Date"></property>
		<property name="book_id" column="book_id" type="string"></property>
	</class>
</hibernate-mapping>